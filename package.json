{
  "name": "devour",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && bun test && bun build ./src/main.ts --outfile=./dist/devour.js --target=bun --minify",
    "debugBuild": "tsc && bun build src/main.ts --sourcemap=linked --outfile=dist/devour.js --target=bun",
    "test": "bun run build && bun dist/devour.js everything path=\"./src/assets/testData/testIngestFile.json\""
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/pg": "^8.11.8",
    "pg": "^8.12.0",
    "sharp": "^0.33.4"
  }
}
